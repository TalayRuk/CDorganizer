<h1>CD listing: </h1>
@if (Model.Count == 0)
{
  <p>We're sorry, There're no CD yet!</p>
  <a href="/add_cds">Add CD</a>
}
else
{
  @foreach (var album in Model)
  {
  <h4>@album.GetTitle()</h4>
  <p>By: @album.GetArtist()</p>
  <p>Price: $@album.GetPrice()</p>
  }
  <a href="/add_cds">Add CD</a>
}
